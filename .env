# Tenant Agent Configuration

# Unique ID for the tenant
TENANT_ID=default

# Port the tenant-agent will listen on
AGENT_PORT=4000

# Log level: info | warn | error | debug
LOG_LEVEL=info

# Path to store logs (can override PATHS.logs)
LOG_PATH=C:\ProgramData\TenantAgent\logs

# Path to tenant-agent binary (optional override)
AGENT_BIN_PATH=C:\Program Files\TenantAgent\bin\tenant-agent.exe

# Auto-update enabled: true | false
AUTO_UPDATE=true

# Update server URL (if using HTTP updates)
UPDATE_URL=https://updates.example.com/tenant-agent
